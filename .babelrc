{
        "presets": [
                "@babel/preset-env",
                ["@babel/preset-react", { "runtime": "automatic" }]
        ],
        "plugins": [
                "babel-plugin-postcss",
                "inline-react-svg",
                "babel-plugin-twin",
                "babel-plugin-macros",
                "babel-plugin-styled-components",
                [
                        "babel-plugin-root-import",
                        {
                                "paths": [
                                        {
                                                "rootPathSuffix": "./",
                                                "rootPathPrefix": "~/"
                                        }
                                ]
                        }
                ]
        ]
}
